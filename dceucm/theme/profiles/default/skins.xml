<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="DCEUCM Theme">

 <object name="dceucm_theme_custom_images"
    meta_type="Filesystem Directory View"
    directory="dceucm.theme:skins/dceucm_theme_custom_images"/>
 <object name="dceucm_theme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="dceucm.theme:skins/dceucm_theme_custom_templates"/>
 <object name="dceucm_theme_styles"
    meta_type="Filesystem Directory View"
    directory="dceucm.theme:skins/dceucm_theme_styles"/>

 <skin-path name="DCEUCM Theme" based-on="Plone Default">
  <layer name="dceucm_theme_custom_images"
     insert-after="custom"/>
  <layer name="dceucm_theme_custom_templates"
     insert-after="dceucm_theme_custom_images"/>
  <layer name="dceucm_theme_styles"
     insert-after="dceucm_theme_custom_templates"/>
 </skin-path>

</object>
